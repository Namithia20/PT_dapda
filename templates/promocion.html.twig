<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}
            <div>
                <img src="{{ asset('images/img-promo.jpg') }}" alt="img-promo"/>
            </div>
            <div>
                <p>Formulario</p>
                {{form_start(form)}}
                    {{ form_widget(form) }}
                {{ form_end(form) }}
            </div>
        {% endblock %}
        {% block javascripts %}
            <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
            <script src="{{ asset('js/promocion.js') }}"></script>
        {% endblock %}
    </body>
</html>